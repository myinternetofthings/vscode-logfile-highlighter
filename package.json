		"type": "git",
		"url": "https://github.com/myinternetofthings/vscode-logfile-highlighter.git"
	},
	"engines": {
		"vscode": ">=0.9.0-pre.1"
	},
	"publisher": "emilast",
  "author": "emilast",
  "contributors": [{
    "name": "myinternetofthings",
    "email": "c00@wp.pl"
  }],
	"contributes": {
		"languages": [{
			"id": "log",
			"aliases": ["Log File", "log"],
